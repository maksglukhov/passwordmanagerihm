<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projet">

    <uses-permission android:name="android.permission.VIBRATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Projet">
        <activity
            android:name=".main.MainActivity"
            android:exported="true"
            android:label="@string/PasswordManager">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".user.CreateUserActivity" android:label="@string/crtUs"></activity>
        <activity android:name=".user.ChooseUserActivity" android:label="@string/choser"></activity>
        <activity android:name=".actions.AddPasswordActivity" android:label="@string/addords"></activity>
        <activity android:name=".actions.AllPasswordsActivity" android:label="@string/allords"></activity>
        <activity android:name=".actions.ChangeProfileActivity" android:label="@string/changile"></activity>
        <activity android:name=".checknchoose.CheckAndActionActivity" android:label="@string/veric"></activity>
        <activity android:name=".actions.ChangePwdActivity" android:label="@string/changadord"></activity>
        <activity android:name=".generatepassword.GeneratePwdActivity" android:label="@string/generord"></activity>
        <activity android:name=".generatepassword.GetGeneratedPasswordActivity" android:label="@string/generatord"></activity>
    </application>

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

</manifest>